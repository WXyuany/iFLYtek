<launch>

  <!-- Run the map server -->
   
  <include file="$(find ucar_controller)/launch/base_driver.launch" > </include>

  <include file="$(find ydlidar)/launch/ydlidar.launch" > </include>

  <node name="map_server" pkg="map_server" type="map_server" args="$(find ucar_nav)/maps/xf_place.yaml" output="screen">
   <param name="frame_id" value="map" />
  </node> 

<!-- <param name="base_global_planner" value="voronoi_planner/VoronoiPlanner"/>  -->
  <!-- <node name="rviz" pkg="rviz" type="rviz" args="-d /home/ucar/ucar_ws/src/test.rviz" output="screen" /> -->
  
  <include file="$(find ucar_nav)/launch/config/amcl/amcl_omni_1.launch" > </include>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <param name="base_global_planner" value="global_planner/GlobalPlanner"/>
    <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
    <rosparam file="$(find ucar_nav)/launch/config/move_base1/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find ucar_nav)/launch/config/move_base1/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find ucar_nav)/launch/config/move_base1/global_planner_params.yaml" command="load" />
    <rosparam file="$(find ucar_nav)/launch/config/move_base1/teb_local_planner_params.yaml" command="load" />
    <rosparam file="$(find ucar_nav)/launch/config/move_base1/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find ucar_nav)/launch/config/move_base1/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find ucar_nav)/launch/config/move_base1/move_base_params.yaml" command="load" />
  </node>

  <!-- <node name="multi-nav" pkg="multi-nav" type="multi-nav" args="$(find ucar_nav)/multi-nav-3.py" output="screen" /> -->
  <!-- rosparam command="load" file="$(find ucar_nav)/launch/config/move_base/my_navigation.yaml"/ -->

</launch>
